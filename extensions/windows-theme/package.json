{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "windows-theme",
  "title": "Windows Theme",
  "description": "Easily toggle your windows theme",
  "icon": "theme-icon.png",
  "author": "c0b41",
  "categories": [
    "Productivity",
    "System",
    "Other"
  ],
  "keywords": [
    "windows",
    "theme"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "toggle-window-theme",
      "title": "Toggle Windows Theme",
      "description": "This command will toggle your current theme",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.35.2",
    "@raycast/utils": "^1.19.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  },
  "platforms": [
    "Windows"
  ]
}
